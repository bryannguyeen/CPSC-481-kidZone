        <Image x:Name="smiley_face_png" Margin="156,323,1431,457" Source="smiley face.png" Stretch="Fill"/>
        <Image x:Name="heart_png" Margin="1426,325,144,455" Source="heart.png" Stretch="Fill"/>
        <Image x:Name="star_png" Margin="788,719,782,61" Source="star.png" Stretch="Fill"/>
